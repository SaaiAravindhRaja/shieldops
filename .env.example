# ShieldOps Environment Configuration
# Copy to .env and fill in your API keys

# LLM Provider Keys (used by Archestra for agent inference)
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
GOOGLE_AI_API_KEY=your-google-ai-key

# Threat Intelligence API Keys (used by threat-intel MCP server)
VIRUSTOTAL_API_KEY=your-virustotal-key
ABUSEIPDB_API_KEY=your-abuseipdb-key

# Database (defaults match docker-compose.yml)
DATABASE_URL=postgresql://archestra:archestra@localhost:5432/archestra

# Archestra (defaults match docker-compose.yml)
ARCHESTRA_URL=http://localhost:9000
